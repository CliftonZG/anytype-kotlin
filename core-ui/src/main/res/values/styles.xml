<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="DefaultDocumentContainerStyle">
        <item name="android:layout_marginTop">@dimen/default_document_item_margin_top</item>
        <item name="android:layout_marginBottom">@dimen/default_document_title_margin_bottom</item>
        <item name="android:paddingStart">@dimen/default_document_item_padding_start</item>
        <item name="android:paddingEnd">@dimen/default_document_item_padding_end</item>
    </style>

    <style name="BlockTextContentStyle">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingStart">@dimen/default_document_content_padding_start</item>
        <item name="android:paddingEnd">@dimen/default_document_content_padding_end</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="BlockHeaderOneContentStyle">
        <item name="android:paddingStart">@dimen/default_document_content_padding_start</item>
        <item name="android:paddingEnd">@dimen/default_document_content_padding_end</item>
        <item name="android:fontFamily">@font/graphik_semibold</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">28sp</item>
        <item name="android:paddingTop">26dp</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BlockHeaderTwoContentStyle">
        <item name="android:paddingStart">@dimen/default_document_content_padding_start</item>
        <item name="android:paddingEnd">@dimen/default_document_content_padding_end</item>
        <item name="android:fontFamily">@font/graphik_semibold</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:textSize">22sp</item>
        <item name="android:paddingTop">19dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BlockHeaderThreeContentStyle">
        <item name="android:paddingStart">@dimen/default_document_content_padding_start</item>
        <item name="android:paddingEnd">@dimen/default_document_content_padding_end</item>
        <item name="android:fontFamily">@font/inter_bold</item>
        <item name="android:background">@null</item>
        <item name="android:textStyle">bold</item>
        <item name="text_size">17sp</item>
        <item name="android:lineSpacingExtra">3sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingTop">19dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BlockTitleContentStyle">
        <item name="android:background">@null</item>
        <item name="android:fontFamily">@font/graphik_semibold</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:lineSpacingExtra">2sp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">34sp</item>
    </style>

    <style name="BlockCheckboxContentStyle">
        <item name="android:background">@null</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BlockPageContentStyle">
        <item name="android:layout_marginStart">4dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="BlockBulletContentStyle">
        <item name="android:background">@null</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">13dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="ToggleContentStyle">
        <item name="android:background">@null</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:layout_marginStart">4dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="BlockNumberedContentStyle">
        <item name="android:background">@null</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:layout_marginStart">13dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="BlockCodeContentStyle">
        <item name="android:inputType">textMultiLine|textNoSuggestions</item>
        <item name="android:background">@null</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:lineSpacingExtra">7sp</item>
        <item name="android:textColor">#2C2B27</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="BlockCodeLanguageMenuStyle">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:lineSpacingExtra">7sp</item>
        <item name="android:drawablePadding">4dp</item>
        <item name="android:drawableEnd">@drawable/ic_turn_into_arrow</item>
        <item name="android:textColor">#ACA996</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="BlockDividerStyle">
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:background">#DFDDD0</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="BlockBookmarkTitleStyle">
        <item name="fontFamily">@font/inter_medium</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#2C2B27</item>
    </style>

    <style name="BlockBookmarkDescriptionStyle">
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:textSize">13sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">2</item>
        <item name="android:textColor">#6C6A5F</item>
    </style>

    <style name="BlockBookmarkUrlStyle">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#2C2B27</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="BlockFileFilenameContentStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">6dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="BlockFileFileSizeContentStyle">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#ACA996</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>

    <style name="BlockVideoStyle">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:inputType">textUri</item>
        <item name="android:textColorHint">#ACA996</item>
        <item name="android:background">@null</item>
        <item name="android:fontFamily">@font/inter_regular</item>
    </style>

    <style name="BlockContactContentStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginStart">28dp</item>
        <item name="android:layout_marginEnd">28dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="BlockHighlightContentStyle">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">17sp</item>
        <item name="android:lineSpacingExtra">3sp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ModalRowTestStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ModalTitleStyle">
        <item name="android:textColor">#2C2B27</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="RoundedBgTextView" parent="@android:style/Widget.TextView">
        <item name="roundedTextHorizontalPadding">2dp</item>
        <item name="roundedTextVerticalPadding">2dp</item>
        <item name="roundedTextDrawable">@drawable/rounded_text_bg</item>
        <item name="roundedTextDrawableLeft">@drawable/rounded_text_bg_left</item>
        <item name="roundedTextDrawableMid">@drawable/rounded_text_bg_mid</item>
        <item name="roundedTextDrawableRight">@drawable/rounded_text_bg_right</item>
    </style>

    <style name="AddBlockOrTurnIntoItemTitleStyle">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="AddBlockOrTurnIntoItemSubtitleStyle">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textColor">#6C6A5F</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="AddBlockOrTurnIntoSectionTitleStyle">
        <item name="android:fontFamily">@font/graphik_medium</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#ACA996</item>
    </style>

    <style name="StyleToolbarTabStyle">
        <item name="android:background">@drawable/tab_style_toolbar_selector</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/selector_style_toolbar_tab_text_color</item>
        <item name="android:fontFamily">@font/graphik_medium</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingEnd">16dp</item>
    </style>

    <style name="BlockStyleGroupDividerStyle">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">#DFDDD0</item>
    </style>

    <style name="BlockStyleToolbarLeftBlockStyle">
        <item name="android:background">@drawable/bg_toolbar_style_block_left_selector</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">48dp</item>
    </style>

    <style name="BlockStyleToolbarMiddleBlockStyle">
        <item name="android:background">@drawable/bg_toolbar_style_block_middle_selector</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">48dp</item>
    </style>

    <style name="BlockStyleToolbarRightBlockStyle">
        <item name="android:background">@drawable/bg_toolbar_style_block_right_selector</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">48dp</item>
    </style>

    <style name="BottomMultiSelectToolbarOptionStyle">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BlockMediaRootLayoutStyle">
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
    </style>

    <style name="ActionModePopupButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/popup_context_menu_height</item>
        <item name="android:paddingStart">@dimen/dp_16</item>
        <item name="android:paddingEnd">@dimen/dp_16</item>
        <item name="android:background">?selectableItemBackground</item>
    </style>

    <style name="DefaultCircleColorStyle">
        <item name="android:layout_width">@dimen/block_style_toolbar_circle_size</item>
        <item name="android:layout_height">@dimen/block_style_toolbar_circle_size</item>
    </style>

</resources>